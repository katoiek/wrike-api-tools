{
  "name": "wrike-api-tools",
  "version": "1.0.0",
  "description": "Wrike API tools with local UI",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/public && cp -r src/views dist/ && cp -r src/public/* dist/public/ 2>/dev/null || true && mkdir -p dist/locales && cp -r locales/* dist/locales/ 2>/dev/null || true",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "jest"
  },
  "keywords": [
    "wrike",
    "api",
    "integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "i18n": "^0.15.1",
    "multer": "^2.0.2",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/i18n": "^0.13.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
